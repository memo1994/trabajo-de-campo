            <h5 class="form-check-h5">
              {{beneficio}}
            </h5>
          </div>
          {{/each}}
        </div>
      <br>
      </div>
      <br>
      <br>
      <div class="col-12" style="border-bottom: 2px solid">
      <br>
        <h4>Necesita valoración médica?</h4>
        <br>
        <a class="btn btn-success" href="#" role="button">SI</a>
        <a class="btn btn-danger" href="#" role="button">NO</a>
